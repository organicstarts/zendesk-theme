<div class="container" style="max-width:1300px;width:100%;position:relative;">
  <div class="ui divider"></div>
  <nav class="sub-nav">
    {{breadcrumbs}}
  </nav>
  
  <div class="row">
    <header class="col-12 col-lg-8 order-2 order-lg-1 align-self-center" id="category-container">
      <h1 class="ui header">
        {{category.name}}
        {{#if category.description}}
        <div class="sub header">{{category.description}}</div>
        {{/if}}
      </h1>
    </header>
    <aside class="col-12 col-lg-4 order-1 order-lg-1 align-self-center">
      {{search scoped=true submit=false}}
      <div class="ui divider d-lg-none d-xl-none"></div>
    </aside>
  </div>
  <div class="ui divider"></div>
  {{#if sections}}
  <section class="row">
    <div class="section col-12 col-md-6 col-lg-4">
      <img class="img-fluid mx-auto d-none" src="{{asset 'lacey-nutrition.png'}}" id="lacey-nutrition">
      <img class="img-fluid mx-auto d-none" src="{{asset 'lacey-instructions.png'}}" id="lacey-instructions">
      <img class="img-fluid mx-auto d-none" src="{{asset 'lacey-wholesale.png'}}" id="lacey-wholesale">
      <img class="img-fluid mx-auto d-none" src="{{asset 'lacey-orders.png'}}" id="lacey-orders">
