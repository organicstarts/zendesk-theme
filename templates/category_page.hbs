          {{/if}}
          <a href="{{url}}" class="pink-text">{{name}}</a>
        </h3>
        {{#if articles}}
        {{#each articles}}
        <a class="item" href="{{url}}" class="article-list-link">
          {{#if promoted}}
          <i class="ui yellow star icon" data-title="{{t 'promoted'}}"></i>
